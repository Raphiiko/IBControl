{
  "build": {
    "beforeDevCommand": "npm run start",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:4200",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "IBControl",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "notification": {
        "all": true
      },
      "fs": {
        "scope": [
          "$RESOURCE/*"
        ]
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "co.raphii.ibcontrol",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [
        "../swagger/*"
      ],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://gist.githubusercontent.com/Raphiiko/1f5fe8b45170011cbbef2c1ed34a8e6c/raw"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDVCNTRFRDlFRjFCNjc5ODkKUldTSmViYnhudTFVVytQcEtEUmlOckNoaUMwVTNSRmR5VXJOeGtYZnRtRGIwZjNDMjNPenJNWGsK"
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 400,
        "width": 600,
        "resizable": false,
        "title": "IBControl",
        "userAgent": "IBControl/0.1.0 (https://github.com/Raphiiko/IBControl)",
        "transparent": true,
        "center": true,
        "theme": "Dark"
      }
    ]
  }
}